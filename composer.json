{
    "name": "syntaxlexx/chatmessenger",
    "description": "Simple one-to-one/group chat messaging tool for Laravel 5, 6, 7, 8, 9 & 10 with Pusher Integration",
    "keywords": [
        "laravel",
        "chat",
        "messenger",
        "messaging",
        "user messaging",
        "chat",
        "pusher"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "/SyntaxLexx",
            "email": "syntaxlexx@gmail.com",
            "homepage": "https://github.com/syntaxlexx",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "illuminate/config": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "adamwathan/faktory": "0.3.*",
        "friendsofphp/php-cs-fixer": "^2.5|^3.51",
        "orchestra/testbench": "^3.0|^4.0|^9.0|^10.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0|^10.5|^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Lexx\\ChatMessenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lexx\\ChatMessenger\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lexx\\ChatMessenger\\ChatMessengerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
